
@{
    ViewBag.Title = "Result";
}

<div class="row">
    <div class="col-md-12 text-center">
        <h1>Congratulations</h1>
    </div>
</div>
<hr />
<div class="row">
    <div class="col-md-12 text-center">
        <h2>You’ve completed the Great Bible Race Guinness World Record Quiz</h2>
    </div>
</div>
<div class="row">
    <div class="col-md-12 text-center">
        <p>Total Correct Answers: @ViewBag.CorrectAnswers/@ViewBag.TotalQuestions</p>
    </div>
</div>
<div class="row">
    <div class="col-md-12 text-center">
        <p>Total Time: @(((TimeSpan)ViewBag.TimeSpan).Hours):@(((TimeSpan)ViewBag.TimeSpan).Minutes):@(((TimeSpan)ViewBag.TimeSpan).Seconds)</p>
    </div>
</div>