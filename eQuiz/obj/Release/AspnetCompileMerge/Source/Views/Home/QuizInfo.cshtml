@model IEnumerable<eQuiz.ViewModels.QuizInfoViewModel>

@{
    ViewBag.Title = "QuizInfo";
}

<h2>QuizInfo</h2>
<hr />

<div class="row">
    <div class="col-md-12">
        <table class="table table-striped">
            <thead><tr><th>Name</th><th>Email</th><th>Correct Answers</th><th>Login Time</th><th>Quiz Time</th><th>IP Address</th></tr></thead>
            <tbody>
                @foreach (var qivm in Model)
                {
                    <tr><td>@qivm.UserFullName</td><td>@qivm.UserEmail</td><td>@qivm.CorrectAnswersCount</td><td>@qivm.LoginTime</td><td>@qivm.QuizTime.Hours Hours : @qivm.QuizTime.Minutes Minutes : @qivm.QuizTime.Seconds Seconds : @qivm.QuizTime.Milliseconds Milliseconds</td><td>@qivm.UserIpAddress</td></tr>
                }
            </tbody>
        </table>
    </div>
</div>
